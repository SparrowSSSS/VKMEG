{
    "name": "VKMEG",
    "description": "Displays an error block in VK Messenger",
    "version": "2.0",
    "manifest_version": 3,
    "icons": {
        "32":"icons/icon32.png",
        "48":"icons/icon48.png",
        "128":"icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [ "https://web.vk.me/*" ],
            "js": [ "app.js" ]
        }
    ],
    "web_accessible_resources": [{
        "matches": ["https://web.vk.me/*"],
        "resources": ["author.jpg", "cross.svg"]
    }]
}